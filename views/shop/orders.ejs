<%- include('../includes/head.ejs') %>
    <link rel="stylesheet" href="/css/shop.css">
    
</head>
<body>
    <%- include('../includes/navigation.ejs') %>
    <main>
    <% if(orders.length <= 0) {%>
        <h1>Nothing there</h1>
        <%}else{%>
        <ul>
            <% orders.forEach(order=>{ %>
                <li>
                    <h1># <%= order.id%></h1>
                    <ul>
                        <% order.products.forEach(items=>{ %>
                            <li><%=items.name%> (<%=items.orderItem.quantity%>)</li>
                       <% })%>
                    </ul>
                </li>

            <%});%>
        </ul>
        <%}%>
    </main>